{
  "manifest_version": 3,
  "name": "Cora Extension",
  "version": "1.0.0",
  "description": "A Chrome extension built with Cora",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/contentScript.js"]
    }
  ]
}

